    "dayjs": "^1.11.13",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "immer": "^10.1.1",
    "next": "14.2.23",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.55.0",
    "swiper": "^11.2.10",
